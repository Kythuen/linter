{
  "name": "@kythuen/linter",
  "version": "0.1.9",
  "description": "Interactively to generate code linter configuration for your code",
  "keywords": [
    "code linter",
    "generator"
  ],
  "author": {
    "name": "Kythuen",
    "email": "616332192@qq.com"
  },
  "license": "MIT",
  "files": [
    "dist",
    ".vscode",
    ".prettierrc",
    ".editorconfig",
    ".husky",
    ".commitlintrc",
    ".lintstagedrc"
  ],
  "bin": {
    "code-linter": "dist/index.js"
  },
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "rimraf dist && tsc -p .",
    "dts": "tsc --declaration --emitDeclarationOnly --outDir dist/types"
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "commander": "^9.4.0",
    "deepmerge": "^4.2.2",
    "execa": "4.1.0",
    "fs-extra": "^10.1.0",
    "gradient-string": "^2.0.1",
    "kolorist": "^1.5.1",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "user-home": "^3.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.5",
    "@types/fs-extra": "^9.0.13",
    "@types/gradient-string": "^1.1.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.3",
    "@types/prettier": "^2.6.3",
    "@types/prompts": "^2.0.14",
    "@types/user-home": "^2.0.0",
    "eslint": "^8.19.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
